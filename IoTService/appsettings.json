{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "PathFile": "C:\\Patients",
  "LOG_FOLDER": "Logs",
  "EitanMedicalUrl": "https://localhost:7000",
  "Communication": {
    "Patient": {
      "API": {
        "SendAllPatients": "api/Patient/AddListPatients",
        "SendPatient": "api/Patient/AddPatient"

      }
    },
    "HeartRateReading": {
      "API": {
        "SendAllHeartRateReadings": "api/HeartRateReading/AddListHeartRateReadings",
        "HeartRateReading": "api/HeartRateReading/AddHeartRateReading"
      }
    }
  }
}
